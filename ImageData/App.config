<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="DbmlBuilderService" type="DbmlBuilder.DbmlBuilderSection, DbmlBuilder" requirePermission="false"/>
  </configSections>
  <DbmlBuilderService defaultProvider="CMSImage">
    <providers>
      <clear/>
      <add name="CMSImage" type="DbmlBuilder.SqlDataProvider, DbmlBuilder" connectionStringName="CMSImage" generatedNamespace="ImageData"/>
