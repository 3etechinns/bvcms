    <add name="CMSImage" connectionString="Data Source=.;Initial Catalog=ImageData;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>
<startup><supportedRuntime version="v2.0.50727"/></startup></configuration>
