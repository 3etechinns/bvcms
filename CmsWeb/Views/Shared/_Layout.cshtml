@using StackExchange.Profiling
<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico?v=2")">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, maximum-scale=1.0">
    <title>@ViewBag.Title</title>
    @ViewExtensions2.GoogleFonts()
    @ViewExtensions2.FontAwesome()
    @ViewExtensions2.jQueryUICss()
    @Fingerprint.Css("/Content/touchpoint/css/app.css")
    <link href="@Url.Content("~/Content/touchpoint/css/print.css")" media="print" rel="stylesheet">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @RenderSection("head", false)
</head>
    <body>
        @Html.Partial("NavBar")
        @Html.Partial("PageHeader")
        <div class="container-fluid" id="main">
            @RenderBody()
        </div>
        @Html.Partial("PageFooter")
        <div class="device-lg visible-lg-block"></div>
        <div class="device-md visible-md-block"></div>
        <div class="device-sm visible-sm-block"></div>
        <div class="device-xs visible-xs-block"></div>
        <div class="device-2xs"></div>
        <div class="modal fade" id="empty-dialog" data-keyboard="false" data-backdrop="static"></div>
        @ViewExtensions2.jQuery()
        @ViewExtensions2.LoDash()
        @ViewExtensions2.Bootstrap3()
        @ViewExtensions2.jQueryUI()
        @ViewExtensions2.Velocity()
        @ViewExtensions2.Moment()
        @ViewExtensions2.jQueryValidation()
        @Fingerprint.Script("/Content/touchpoint/lib/jquery-globalize/js/globalize.js")
        <script src="@Url.Content(ViewExtensions2.jqueryGlobalizeCulture)" type="text/javascript"></script>
        @Fingerprint.Script("/Content/touchpoint/lib/jquery-validate-globalize/js/jquery.validate.globalize.js")
        @Fingerprint.Script("/Content/touchpoint/lib/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js")
        @Fingerprint.Script("/Content/touchpoint/lib/bootstrap-sweetalert/js/sweet-alert.min.js")
        @Fingerprint.Script("/Content/touchpoint/lib/bootstrap-bootbox/js/bootbox.min.js")
        @Fingerprint.Script("/Content/touchpoint/lib/mousetrap/js/mousetrap.min.js")
        @Fingerprint.Script("/Content/touchpoint/lib/jquery-blockUI/js/jquery.blockUI.js")
        @Fingerprint.Script("/Content/touchpoint/js/main.js")
        @Fingerprint.Script("/Content/touchpoint/js/export-toolbar.js")
        @Fingerprint.Script("/Content/touchpoint/js/extra-value.js")
        @Fingerprint.Script("/Content/touchpoint/js/form-ajax.js")
        @Fingerprint.Script("/Content/touchpoint/js/search/search-add.js")
        @Html.Partial("DatePickerDefaults")
        @RenderSection("scripts", false)
        @Html.Partial("SearchBox")
        @MiniProfiler.RenderIncludes()
    </body>
</html>
