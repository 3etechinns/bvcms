<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="shortcut icon" href="@Url.Content("~/favicon.ico?v=2")">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, maximum-scale=1.0">
    <title>@ViewBag.Title</title>
    @ViewExtensions2.GoogleFonts()
    @ViewExtensions2.FontAwesome()
    @ViewExtensions2.TouchpointBootstrap3Css()
    @ViewExtensions2.jQueryUICss()
    <link href="@Url.Content("~/Content/touchpoint/bootstrap-sweetalert/css/sweet-alert.css")" rel="stylesheet">
    @Fingerprint.Css("/Content/touchpoint/bootstrap-datepicker/datepicker3.css")
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <link href="@Url.Content("~/Content/touchpoint/css/main.css")" rel="stylesheet">
    <link href="@Url.Content("~/Content/touchpoint/css/print.css")" media="print" rel="stylesheet">
    @RenderSection("head", false)
</head>
    <body>
        @Html.Partial("NavBar")
        <div class="container-fluid" id="main">
            @RenderBody()
        </div>
        <div id="contact-footer">
            <div class="texture" style="background: url(/Content/touchpoint/images/hexagon.png) repeat;"></div>
            <ul class="list-inline">
                <li>@ViewExtensions2.DbSetting("nameofchurch", "Name of Church")</li>
                <li><i class="fa fa-phone"></i>&nbsp;&nbsp;<a href="tel:+@ViewExtensions2.DbSetting("ChurchPhone", "phone").FmtFone()">@ViewExtensions2.DbSetting("ChurchPhone", "phone").FmtFone()</a></li>
                <li><i class="fa fa-envelope"></i>&nbsp;&nbsp;<a href="mailto:@DbUtil.AdminMail">@DbUtil.AdminMail</a></li>
            </ul>
        </div>
        <div id="footer" class="container-fluid hidden-print">
            <div class="row">
                <div class="col-xs-6">
                    <img id="powered-by" src="@Url.Content("~/Content/touchpoint/images/powered-by.png")" />
                </div>
                <div class="col-xs-6 text-right">
                    &copy; 2014 TouchPoint Software, LLC.
                </div>
            </div>
        </div>
        <div class="device-lg visible-lg-block"></div>
        <div class="device-md visible-md-block"></div>
        <div class="device-sm visible-sm-block"></div>
        <div class="device-xs visible-xs-block"></div>
        <div class="device-xss"></div>
        @ViewExtensions2.jQuery()
        @ViewExtensions2.LoDash()
        @ViewExtensions2.Bootstrap3()
        @ViewExtensions2.jQueryUI()
        @Fingerprint.Script("/Scripts/touchpoint/bootstrap-datepicker/bootstrap-datepicker.js")
        <script src="@Url.Content("~/Scripts/touchpoint/bootstrap-sweetalert/sweet-alert.min.js")"></script>
        <script src="@Url.Content("~/Scripts/touchpoint/bootstrap-bootbox/bootbox.min.js")"></script>
        <script src="@Url.Content("~/Scripts/touchpoint/jquery-blockUI/jquery.blockUI.js")"></script>
        <script src="@Url.Content("~/Scripts/touchpoint/velocity/velocity.min.js")"></script>
        <script src="@Url.Content("~/Scripts/touchpoint/js/main.js")"></script>
        <script type="text/javascript">
            $.dateFormat = '@Util.jQueryDateFormat';
        </script>
        @RenderSection("scripts", false)
        @Html.Partial("SearchBox")
    </body>
</html>