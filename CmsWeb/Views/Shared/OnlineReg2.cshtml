@if ((bool?)ViewBag.hasshell == true)
{
    @Html.Action("Index", "MobileAppMenu", new { area = "" })
    @Html.Raw((string)ViewBag.top)
    @RenderSection("head", false)
    @RenderBody()
    @Html.Raw((string)ViewBag.bottom)
    @ViewExtensions2.jQuery()
    @ViewExtensions2.Bootstrap3()
    @ViewExtensions2.FontAwesome()
    @ViewExtensions2.LoDash()
    @Fingerprint.Script("/Scripts/bundle.onlineregister.js")
    @RenderSection("scripts", false)
}
else
{
    @:<!DOCTYPE html>
    <html>
    <head>
        <title>Online Registration</title>
        <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
        @Html.Raw(ViewExtensions2.Bootstrap3Css())
        @ViewExtensions2.FontAwesome()
        @RenderSection("head", false)
    </head>
    <body>
      @*        @Html.Action("Index", "MobileAppMenu", new { area = "" })
    This breaks when anybody else uses the return Message("message") *@
      @Html.Raw((string)ViewBag.top)
      <div class="container">
        @RenderBody()
      </div>
      @Html.Raw((string)ViewBag.bottom)
      @ViewExtensions2.jQuery()
      @ViewExtensions2.Bootstrap3()
      @ViewExtensions2.LoDash()
      @Fingerprint.Script("/Scripts/bundle.onlineregister.js")
      @RenderSection("scripts", false)
    </body>
</html>
}