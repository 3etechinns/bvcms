<!DOCTYPE html>
<html lang="@System.Globalization.CultureInfo.CurrentUICulture.Name">
<head>
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon" />
    @ViewExtensions2.GoogleFonts()
    @ViewExtensions2.NewStyles()
    @ViewExtensions2.FontAwesome()
    @RenderSection("head", required: false)
    @ViewExtensions2.FixupsCss()
    @ViewExtensions2.jQuery()
    @ViewExtensions2.Bootstrap()
    <script src="@Url.Content(ViewExtensions2.Globalize)" type="text/javascript"></script>
    <script src="@Url.Content(ViewExtensions2.GlobalizeCulture)" type="text/javascript"></script>
    @Fingerprint.Script("/Scripts/bundle.main2.js")
    @Html.Partial("DatePickerDefaults2")
    <script>
        $(function () {
            $('#addorg').click(function (e) {
                e.preventDefault();
                bootbox.alert("Add Organization not available from this page yet in the new UI.");
            });
        });
    </script>
    @RenderSection("scripts", required: false)
</head>
<body>
    <div class="page-container">
        @Html.Partial("Notices")
        @Html.Partial("NewHeader3")
        @RenderBody()
    </div>
    @Html.Partial("Footer")
    @Html.Partial("Dialogs2")
    @RenderSection("PopupsPlaceholder", required: false)
        <div id="loading-indicator">
            <img src="/Content/img/ajax-loader.gif" />
        </div>
</body>
</html>
