@if ((bool?)ViewData["hasshell"] == true)
{
@Html.Raw((string)ViewData["top"])
@RenderSection("head", false)
@RenderBody()
@ViewExtensions2.jQuery()
@ViewExtensions2.jQueryUI()
@Fingerprint.Script("/Scripts/bundle.onlineregister.js")
@RenderSection("scripts", false)
@Html.Raw((string)ViewData["bottom"])
}
else
{
@:<!DOCTYPE html>
<html>
<head>
<title>Online Registration</title>
@Html.Raw(Util.PickFirst((string)ViewData["header"], @"
<meta http-equiv='Content-Type' content='text/html; charset=iso-8859-1' />" +
ViewExtensions2.jQueryUICss() +
@"<link href='/Content/styles/onlinereg.css?v=7' rel='stylesheet' type='text/css' />
"))
@RenderSection("head", false)
</head>
<body>
@Html.Raw((string)ViewData["top"])
@RenderBody()
@Html.Raw((string)ViewData["bottom"])
@ViewExtensions2.jQuery()
@ViewExtensions2.jQueryUI()
@Fingerprint.Script("/Scripts/bundle.onlineregister.js")
@RenderSection("scripts", false)
</body>
</html>
}