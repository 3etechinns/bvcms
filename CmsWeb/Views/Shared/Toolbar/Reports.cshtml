                <li><a href='/Export2/Excel/Library/@qid'>Library</a></li>
                <li><a href='/Export2/Excel/Church/@qid'>Other Churches</a></li>
            </ul>
            <ul class="span4 unstyled pull-left">
                <li class="header">Reports</li>
                @if ((bool?)ViewBag.OrganizationContext == true)
                {
                    <li><a href='/Reports2/Contacts/@qid' target="report" title="Report for Robo-calling Contacts">Contact Report</a></li>
                    <li class="hideAlt"><a href='/Reports2/Contacts/@qid' target="report">Contact Report (by addr)</a></li>
                }
                else
                {
                    <li><a href='/Reports2/Contacts/@qid' target="report" title="Report for Robo-calling Contacts">Contact Report</a></li>
                    <li class="hideAlt"><a href='/Reports2/Contacts/@qid' target="report">Contact Report (Sorted by Address)</a></li>
                }
                <li><a href='/Reports2/Family/@qid' target="report">Family Report</a></li>
                <li><a href='/Reports2/Prospect/@qid' target="report">Inreach/Outreach</a></li>
                <li><a href='/Reports2/Prospect/@qid' target="report">Inreach/Outreach with Form</a></li>
