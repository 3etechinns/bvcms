                {
                    <li><a href='/Reports2/Roster1/@qid' target="report">Roster Report</a></li>
                    <li><a href='/Reports2/Registration/@qid' target="report">Registration Report</a></li>
                }
                else
                {
                    <li><a href='/Reports2/Registration/@qid' target="report">Registration Rpt</a></li>
                }
                <li><a href='/Reports2/WeeklyAttendance/@qid' target="report">title="General Attendance Stats">Weekly Attend</a></li>
                

                <li class="header"><br />Directories</li>
                <li><a href="/Reports2/FamilyDirectoryCompact/@qid">Compact Directory</a></li>
                <li><a href="/Reports2/FamilyDirectory/@qid">Family Directory</a></li>
                <li><a href="/Reports2/PictureDirectory/@qid">Picture Directory</a></li>

                @if ((bool?)ViewBag.OrganizationContext == true)
                { 
                    <li class="header"><br />Rollsheets</li>
                    <li><a id="RollsheetLink" href='#' title="Rollsheet Report">Rollsheet Report</a></li>
                    <li><a id="RallyRollsheetLink" href='#' title="Rally Rollsheet Report">Rally Rollsheet</a></li>
                }
            </ul>
            <ul class="span4 unstyled pull-left">
                <li class="header"> Address Labels </li>
                <li><a href='/Reports2/AveryAddress/@qid' class="dialog-options" data-target="#choose-format">Address</a></li>
                <li><a href="/Reports2/EmployerAddress/@qid">Employer</a></li>
                <li><a href='/Reports2/RollLabels/@qid' class="dialog-options" data-target="#choose-format">Continous Roll Labels</a></li>

