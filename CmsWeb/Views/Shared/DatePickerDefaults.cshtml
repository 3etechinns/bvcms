<script type="text/javascript">
    $.dateFormat = '@Util.jQueryDateFormat';
    $(function () {
        $.datepicker.setDefaults({
            dateFormat: $.dateFormat,
            changeMonth: true,
            changeYear: true,
            yearRange: "1925:2015",
            showOn: "button",
            buttonImage: "/images/calendar.gif",
            buttonImageOnly: true
        });
    });
</script>
