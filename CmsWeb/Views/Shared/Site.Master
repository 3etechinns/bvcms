</head>
<body>
<div>
    
    <script src="/Content/js/jquery-1.4.1.min.js" type="text/javascript"></script>    
    <script src="/Content/js/jquery-ui-1.8rc1.custom.min.js" type="text/javascript"></script>
    <script src="/Content/js/superfish.js" type="text/javascript"></script>
    <script src="/Content/js/supersubs.js" type="text/javascript"></script>    
    <script src="/Content/js/jquery.bgiframe.min.js" type="text/javascript"></script>    
    <script src="/Content/js/hoverIntent.js" type="text/javascript"></script>    
    <script src="/Content/js/jQuery-cookie.js" type="text/javascript"></script>    
    <script src="/Content/js/jQuery.blockUI.js" type="text/javascript"></script>    
    <script src="/Content/js/jquery.dimensions.min.js" type="text/javascript"></script>
    <script src="/Scripts/ExportToolBar.js?v=2" type="text/javascript"></script>    
    <script type="text/javascript">
        $(document).ready(function() {
            $("ul.sf-menu").supersubs({
                minWidth: 8,
                maxWidth: 27,
                extraWidth: 1,
                autoArrows: false
            }).superfish();
        });
        function EnterSubmit(e, myform) {
            var key = window.event ? e.keyCode : e.which;
            if (key == 13) {
                var s = $('#SearchText')[0];
                if (s.value != "Quick Search")
                    window.location = '/Search.aspx?name=' + s.value;
            }
            return true;
        }
    </script>
</div>
<div>
    <% if (((string)Application["getoff"]).HasValue())
       { %>
    <%= "<span class='urgentNotice'>{0}</span>".Fmt(Application["getoff"])%>
    <% } %>
    <div id="CommonHeader">
        <div>
            <%= DbUtil.Header() %>
            <div id="CommonHeaderUserArea">
                <% Html.RenderPartial("LogOnUserControl"); %>
