@using CmsWeb
@model object 
@{
    var items = (ViewData["items"] as IEnumerable<SelectListItem> ?? new List<SelectListItem>()).ToList();
    if (Model != null)
    {
        var selected = items.FirstOrDefault(ii => ii.Value == Model.ToString());
        if (selected != null)
        {
            selected.Selected = true;
        }
    }
}
<div class="form-group">
    @Html.LabelFor(m => m)
    <div class="form-group">
        @Html.DropDownListFor(m => m, items, new { @class = "form-control" })
        @Html.ValidationMessageLabelFor(m => m)
    </div>
</div>
