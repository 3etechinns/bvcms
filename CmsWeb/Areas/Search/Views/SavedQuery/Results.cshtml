@model CmsWeb.Areas.Search.Models.SavedQueryModel
<div class="pagination pagination-mini">
    @Html.Partial("Pager3", Model.Pager)
</div>
<table id="saved-query" class="table table-bordered table-condensed">
    <thead>
        <tr>
            <th>@Model.Pager.SortLink("Description")</th>
            <th>@Model.Pager.SortLink("Owner")</th>
            <th>@Model.Pager.SortLink("Public")</th>
            <th>@Model.Pager.SortLink("Last Run")</th>
            <th>@Model.Pager.SortLink("Count")</th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model.FetchQueries())
        {
            Html.RenderPartial("Row", r);
        }
    </tbody>
</table>
<div class="pagination pagination-mini">
    @Html.Partial("Pager3", Model.Pager)
</div>
