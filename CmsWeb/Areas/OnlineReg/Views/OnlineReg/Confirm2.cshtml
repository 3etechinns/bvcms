@using System.Web.Optimization
@{
    Layout = "/Views/Shared/onlinereg.cshtml";
    ViewBag.Title = "Online Registration";
}

@section scripts
{
@Scripts.Render("~/bundles/single/onlinereg-js")
}

<div class="confirm">
    <h2>Transaction Completed</h2>
    <p>
        Thank you for your payment of @ViewData["Amount"] for @ViewData["Desc"].<br />
        Your balance is @ViewData["AmountDue"].<br />
        You should receive a confirmation email shortly.
    </p>

</div>
