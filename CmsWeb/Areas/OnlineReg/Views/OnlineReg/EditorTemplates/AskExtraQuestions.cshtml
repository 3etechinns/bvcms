@using CmsData.Registration
@model CmsWeb.Models.OnlineRegPersonModel
@{
    var ask = (AskExtraQuestions) ViewBag.ask;
    var name = Html.NameFor2(m => m.ExtraQuestion[ask.UniqueId]);
}
@for (var i = 0; i < ask.list.Count; i++)
{
    var a = ask.list[i];
    <div class="form-group">
        @Html.Label(a.Question, new { @class = "control-label col-sm-3 labelsize" })
        <div class="col-sm-9">
            <input type="hidden" name="@(name)[@i].Key" value="@a.Question" />
            <input type="text" name="@(name)[@i].Value" value="@Model.ExtraQuestionValue(ask.UniqueId, a.Question)" class="form-control" />
            <div>@Html.ValidationMessageFor(m => m.ExtraQuestion[ask.UniqueId][a.Question])</div>
        </div>
    </div>
}
