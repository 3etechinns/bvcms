@using CmsWeb
@model CmsWeb.Areas.Reports.Models.AttendanceDetailModel

@{
    ViewBag.Title = "AttendanceDetail";
    Layout = ViewExtensions2.Layout();
}

<h2>AttendanceDetail</h2>

    <table class="@ViewExtensions2.GridClass">
    <thead>
    <tr>
    <th class="left">OrgName</th>
    <th class="left">Leader</th>
    <th class="left">Date</th>
    <th class="right">Present</th>
    <th class="right">Guests</th>
    <th class="right">OutTowners</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var m in Model.FetchMeetings())
    {
        <tr>
        <td>@m.OrgName</td>
        <td>@m.Leader</td>
        <td><a href="/Meeting/@m.MeetingId">@m.Date</a></td>
        <td class="right">@m.Present</td>
        <td class="right">@m.Visitors</td>
        <td class="right">@m.OutTowners</td>
        </tr>
    }
    </tbody>
    </table>

