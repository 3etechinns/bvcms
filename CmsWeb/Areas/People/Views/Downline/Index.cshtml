@using UtilityExtensions
@model CmsWeb.Areas.People.Models.DownlineModel
@{
  Layout = ViewExtensions2.TouchPointLayout();
  ViewBag.Title = "Downline";
  var name = Model.Name;
  ViewBag.PageHeader = "Downline for {0} ({1:N0}) in {2}".Fmt(name, Model.DownlineCount, Model.Category);
}
<div class="box box-responsive">
  <div class="box-content">
    <form class="ajax" data-results="#results" method="post" action="/Downline/Results">
      @Html.HiddenFor(m => m.CategoryId)
      @Html.HiddenFor(m => m.DownlineId)
      @Html.Partial("Results", Model)
    </form>
  </div>
</div>
