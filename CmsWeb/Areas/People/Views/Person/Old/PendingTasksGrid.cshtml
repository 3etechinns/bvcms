@model IEnumerable<CmsWeb.Models.TaskModel.TasksAbout>
@if (Model.Count() > 0)
{
<table class="grid">
    <thead>
    <tr>
        <th>Task</th>
        <th>Date</th>
        <th>Assigned to</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var t in Model)
    { 
        <tr>
            <td><a href="@t.link">@t.Desc</a></td>
            <td>@t.AssignedDt</td>
            <td><a href="/Person/Index/@t.AssignedId">@t.AssignedTo</a></td>
        </tr>
    }
    </tbody>
</table>
}