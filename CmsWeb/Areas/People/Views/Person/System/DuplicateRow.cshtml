@using UtilityExtensions
@model CmsData.Person.Duplicate
<tr>
    <td><a href="/Person2/@Model.PeopleId">@Model.First</a></td>
    <td>@Model.Nick</td>
    <td>@Model.Middle</td>
    <td>@Model.Last</td>
    <td>@Model.Maiden</td>
    <td>@Util.FormatBirthday(Model.BYear, Model.BMon, Model.BDay)</td>
    <td>@Model.Email<br/>@Model.Member</td>
    <td>@Model.FamAddr<br />@Model.PerAddr</td>
</tr>

