@using System.Web.Optimization
@using UtilityExtensions
@using CmsWeb
@model CmsWeb.Models.MeetingModel
@{
    ViewBag.Title = "Meeting Tickets";
    Layout = "~/Views/Shared/SiteLayout.cshtml";
}

@section head
{
	<style>
	   div.bgred { background-color: pink; }
	   div.bgyellow { background-color: yellow; }
	</style>
}
@section scripts
{
@Scripts.Render("~/bundles/single/meeting-ticket-js")
}
<embed src="/Content/zylo.wav" autostart="false" width="0" height="0" id="zylo" enablejavascript="true">
<embed src="/Content/ding.wav" autostart="false" width="0" height="0" id="ding" enablejavascript="true">

<form method="Post">
    <table>
        <tr>
        	<td id="meeting">
                @Html.Partial("TicketMeeting", Model)
            </td>
        	<td valign="bottom" style="margin-left:1em">
        		Wand Target<br />
        		@Html.TextBox("wandtarget") 
            </td>
        </tr>
    </table>
</form>
<hr />
<div id="mark">
	
</div>
