<table cellpadding="4" class="grid">
<thead>
<tr><td>@pager</td></tr>
<tr>
<td><a href="#" class="sortable">Description</a></td>
<td><a href="#" class="sortable">Owner</a></td>
<td><a href="#" class="sortable">Public</a></td>
<td></td>
</tr>
</thead>
<tbody>
@foreach (var r in Model.FetchQueries())
{
    Html.RenderPartial("Row", r);
}
</tbody>
<tfoot><tr><td>@pager</td></tr></tfoot>
</table>
