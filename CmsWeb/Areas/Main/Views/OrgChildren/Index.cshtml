@model CmsWeb.Models.OrgChildrenModel
@using CmsWeb
@{
    Layout = Fingerprint.Layout();
    ViewBag.Title = "OrgChildren";
}
@section head
{
<style>
    td.wide100 * 
    {
        width: 98%;
    }
</style>
}
@section scripts
{
    @Fingerprint.Script("/Scripts/Org/orgChildren.js")
}
