@model CmsWeb.Models.OrgChildrenModel
@using System.Web.Optimization
@using CmsWeb
@{
    Layout = "/Views/Shared/SiteLayout.cshtml";
    ViewBag.Title = "OrgChildren";
}
@section scripts
{
@Scripts.Render("~/bundles/single/OrgChildren-js")
}
@section head
{
<style>
    td.wide100 * 
    {
        width: 98%;
    }
</style>
}
@if (Model.canedit)
{
@Html.Hidden("canedit", "true")
}
<form id="groupsform" action="/OrgChildren/Update" method="post">
@Html.Partial("Form", Model)
</form>


