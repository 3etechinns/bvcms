@using CmsWeb
@model IEnumerable<CmsWeb.Models.TaskModel.IncompleteTask>
@if (Model.Count() > 0)
{
<table class="@ViewExtensions2.GridClass">
    <thead>
    <tr>
        <th>Task</th>
        <th>Date</th>
        <th>About</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var t in Model)
    { 
        <tr>
            <td><a href="@t.link">@t.Desc</a></td>
            <td>@t.AssignedDt</td>
            <td><a href="/Person2/@t.AboutId">@t.About</a></td>
        </tr>
    }
    </tbody>
</table>
}