@model CmsWeb.Models.SearchContactModel
@if(Model.Count == 0)
{ 
<tr><td colspan="5">No matching records.</td></tr>
} 
@foreach (var c in ViewData.Model.ContactList())
{ 
<tr>
    <td><a href="javascript:AddSourceContact(@c.ContactId)">select</a></td>
    <td>@c.ContactDate.ToShortDateString()</td>
    <td>@c.ContactReason</td>
    <td>@c.TypeOfContact</td>
    <td>@Model.GetContacteeList(c.ContactId)</td>
</tr>
}
<tr><td colspan="5">
@Html.Hidden("Count",Model.Count)
@Html.Hidden("Sort",Model.Sort)
</td></tr>

