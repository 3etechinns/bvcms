@model CmsData.RegSettings.OrgFee
@using CmsWeb
    @using (var ci = Html.BeginCollectionItem("OrgFees"))
    {
      <li>@Html.Raw(ci.hiddenindex)
          <img align="top" src="/images/Up-down.png" alt="" />
          @Html.TextBoxFor(m => m.OrgId, new { @class = "short" })
          <a class="del" href="#"><img align="top" src="/images/delete.gif" title="remove this item" /></a>
        <ul>
          <li><div class="sgfee">Fee:</div> @Html.TextBoxFor(m => m.Fee) </li>
        </ul>
      </li>
    }
