@model CmsWeb.Areas.Dialog.Models.TransactionHistoryModel
@using UtilityExtensions
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>History</title>
    @Html.Raw(SquishIt.Framework.Bundle.Css()
                .Add("/Content/jquery-ui-1.8.13.custom.css")
                .Add("/Content/site.css")
                .Add("/Content/Style2.css")
        .Render("/Content/TransactinoHistory_#.css"))
    @Html.Raw(SquishIt.Framework.Bundle.JavaScript()
        .Add("/Content/js/jquery-1.5.2.js")
        .Add("/Content/js/jquery.jeditable.js")
        .Add("/Scripts/TransactionHistory.js")
        .Render("/Content/TransactionHistory_#.js"))
</head>
<body>
    <div>
        <p>@Model.Name<br />
            @Model.Org</p>
        <table id="attends" class="grid" cellpadding="5">
            <thead>
                <tr>
                    <th> </th>
                    <th> Flag </th>
                    <th> Meeting Date </th>
                    <th> Attend Type </th>
                    <th> Member Type </th>
                    <th> Other Attends </th>
                    <th> Other OrgId </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var a in Model.FetchAttends())
                {
                    <tr>
                        <td>@a.Indicator </td>
                        <td>@a.AttendanceFlag </td>
                        <td>@a.MeetingDate.ToString("M/d/yy h:mmt") </td>
                        <td>@a.AttendType </td>
                        <td>@a.MemberType </td>
                        <td>@a.OtherAttends </td>
                        <td>@a.OtherOrgId </td>
                    </tr>
                }
            </tbody>
        </table>
        <div>&nbsp;</div>
        @{Html.RenderPartial("History", Model.FetchHistory()); }
    </div>
</body>
</html>
