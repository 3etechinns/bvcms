@model CmsWeb.Areas.Dialog.Models.TransactionHistoryModel
@using CmsWeb
@using UtilityExtensions
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <title>History</title>
<link rel="stylesheet" type="text/css"  href="/Content/jquery-ui-1.8.9.custom.css" />
<link rel="stylesheet" type="text/css"  href="/Content/site.css" />
<link rel="stylesheet" type="text/css"  href="/Content/Style2.css" />

@Html.Partial("CombinedJs")
@Html.Partial("DatePickerDefaults")
@Html.Script("/Scripts/TransactionHistory.js")
</head>
<body>
    <div>
        <p>@Model.Name<br />
            @Model.Org</p>
        <table id="attends" class="grid" cellpadding="5">
            <thead>
                <tr>
                    <th> </th>
                    <th> Flag </th>
                    <th> Wk </th>
                    <th> Sch </th>
                    <th> Day </th>
                    <th> Meeting Date </th>
                    <th> Attend Type </th>
                    <th> Member Type </th>
                    <th> Other Attends </th>
                    <th> Other OrgId </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var a in Model.FetchAttends())
                {
                    <tr>
                        <td>@a.Indicator </td>
                        <td>@a.AttendanceFlag </td>
                        <td>@a.MeetingDate.GetWeekNumber()</td>
                        <td>@a.ScheduleType</td>
                        <td>@a.MeetingDate.DayOfWeek.ToString().Substring(0,3)</td>
                        <td>@a.MeetingDate.ToString("g") </td>
                        <td>@a.AttendType </td>
                        <td>@a.MemberType </td>
                        <td>@a.OtherAttends </td>
                        <td>@a.OtherOrgId </td>
                    </tr>
                }
            </tbody>
        </table>
        <div>&nbsp;</div>
        @{Html.RenderPartial("History", Model.FetchHistory()); }
    </div>
</body>
</html>
