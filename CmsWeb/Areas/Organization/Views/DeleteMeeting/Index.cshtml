@model CmsWeb.Areas.Org2.Dialog.Models.DeleteMeeting
<div class="modal-header">
  <a class="close" data-dismiss="modal">x</a>
  <h3>Delete Meeting</h3>
</div>
<div class="modal-body">
  @Html.Hidden("callback", "ReloadMeetings")
  @Html.HiddenFor(m => m.Id)
  @Html.DisplayFor(m => m.Count, new { Label="Number of Attends"})
  Are you sure you want to delete this meeting?
</div>
<div class="modal-footer">
  <a href="#" class="btn btn-success" data-dismiss="modal">Cancel</a>
  <a href="/DeleteMeeting/Process" class="btn btn-primary ajaxreloader">OK</a>
</div>