@model CmsData.Meeting
@{var isedit = User.IsInRole("Edit");}
@foreach (var c in Model.GetMeetingExtras())
{
<tr>
    <td>@c.Field</td>
    @if (isedit)
    {
    <td><span id="s-@c.Field.@c.MeetingId" class="@(c.DataType == "text" ? "editarea" : "editline")">@c.Data</span></td>
    <td><a href="#" class="deleteextra" field="@c.Field"><img src="/Content/images/delete.gif" /></a></td>
    }
    else
    {
    <td>@c.Data</td>
    <td></td>
    }
</tr>
}
