@using CmsWeb
@model string
<a class="notifylist" href="/Organization/NotifyIds/@ViewBag.OrgId?field=@ViewBag.field" data-field="@ViewBag.field" data-target="#usersDialog">
<div class="block border">
<b>choose users to notify (confirmation comes from first)</b>
@{  var q = from p in ViewExtensions2.PeopleFromPidString(Model)
            select new { p.FromEmail, p.PeopleId };
    foreach (var p in q)
    {
    <div>@p.FromEmail &mdash; (@p.PeopleId)</div>
    }
}
</div>
</a>