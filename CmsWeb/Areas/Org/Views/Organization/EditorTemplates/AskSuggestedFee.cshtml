@model CmsData.Registration.AskSuggestedFee
@using CmsWeb
@using (var ci = Html.BeginCollectionItem(Model.Name))
{
	<li class="movable type-@Model.Type">@Html.Raw(ci.hiddenindex)
		@Html.HiddenFor(m => m.Type)
        <span style="cursor: pointer;" data-toggle="popover" data-content="Allows the final fee to be adjusted to any amount (including zero)." data-original-title="AskSuggestedFee"><i class="fa fa-info-circle"></i></span>&nbsp;&nbsp;<label class="keyword big tip">
			AskSuggestedFee
            @Html.Partial("EditorTemplates/QuestionsMenu") 
        </label>
		<ul>
			<li> <span class="keyword2">Label:</span> @Html.TextBoxFor(m => m.Label, new {@class = "long"})</li>
		</ul>
	</li>
}