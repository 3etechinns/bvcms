@model CmsWeb.Areas.Org.Models.OrganizationModel
@{
  Layout = "~/Views/Shared/SiteLayout2.cshtml";
  ViewBag.Title = "Organization";
  ViewBag.OrgMemberContext = true;
  var o = Model.Org;
}
@section head
{
  @Fingerprint.Css("/Content/styles/organization.css")
  @Fingerprint.Css("/Content/css/bootstrap-datetimepicker.css")
}
@section scripts
{
  @ViewExtensions2.CKEditor()
  @Fingerprint.Script("/Scripts/bundle.org.js")
  @Fingerprint.Script("/Scripts/Bootstrap/orgOrgMemberDialog2.js")
  @Fingerprint.Script("/Scripts/Org/bootstrap-datetimepicker.min.js")
}
@Html.Hidden("refreshed", "no")
@Html.Hidden("OrganizationId")
<div class="container">
  <div class="row-fluid" id="profile-header">
    <div class="span8">
      <form id="orginfoform" class="form form-horizontal non-modal ajax" method="post">
        @Html.Partial("Settings/Main", Model)
      </form>
    </div>
    <div class="span4">
      @Html.Partial("Toolbar/Toolbar")
    </div>
  </div>
  <div class="row-fluid">
    <div class="span12">
      <div id="tab-area" class="tabbable primary-tabs">
        <ul class="nav nav-tabs">
          <li class="active"><a href="#Members-tab" data-toggle="tab">People</a></li>
          <li><a href="#Settings-tab" data-toggle="tab" class="ajax">Settings</a></li>
          <li><a href="#Meetings-tab" data-toggle="tab" class="ajax">Meetings</a></li>
        </ul>
        <div class="tab-content">
          <div id="Members-tab" class="tab-pane active" data-link="/Organization/People">
            @Html.Partial("People", Model)
          </div>
          <div id="Settings-tab" class="tab-pane" data-link="/Organization/Settings/@Model.Id" 
            data-init="showHideRegTypes" data-init2="SettingFormsInit">
          </div>
          <div id="Meetings-tab" class="tab-pane" data-link="/Organization/Meetings">
            @Helper.HiddenId("Id", Model.Id)
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
@Html.Partial("Dialogs", Model)