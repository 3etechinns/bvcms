@using System.Text
@using CmsWeb
@{
  ViewBag.Title = "RegSetting";
  Layout = ViewExtensions2.Layout();
}
<div class="roundbox cell">
  <h2>Registration Settings for <a href="/Org/@ViewData["OrganizationId"]">@ViewData["orgname"]</a></h2>
  <form action="/RegSettings/Edit/@ViewData["OrganizationId"]" method="post">
    <input id="edit" type="submit" class="bt" value="Edit Text" />
    <a href="/RegSettings/ConvertFromMdy/@ViewBag.OrganizationId" class="bt">Convert From MDY</a>
    <a href="/RegSettings/ConvertFromDmy/@ViewBag.OrganizationId" class="bt">Convert From DMY</a>
    @{ var lines = (string[])ViewData["lines"]; }
    <input type="hidden" name="regsetting" value="@ViewData["regsetting"]" />
  </form>
</div>
<p style="color: red">@TempData["error"]</p>
@{
  var sb = new StringBuilder();
  for (var i = 0; i < lines.Length; i++)
  {
    sb.AppendFormat("  {0,3}\t{1}\n", i + 1, lines[i]);
  }
}
<pre>
@sb.ToString()
</pre>