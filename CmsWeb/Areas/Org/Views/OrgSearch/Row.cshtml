@model CmsWeb.Areas.Search.Models.OrgSearchModel.OrganizationInfo
@{ var o = Model; }
<tr>
  <td class="tip" title="@o.ToolTip"><img src="/Content/images/group.png" /></td>
  <td><a href="/Organization/@o.Id">@o.OrganizationName</a></td>
  <td><a href="/Person2/@o.LeaderId">@o.LeaderName</a></td>
  <td class="right">@o.MemberCount</td>
  @if (o.PrevMemberCount == 0 && o.ProspectCount == 0)
  {
    <td></td>
  }
  else
  {
    <td class="right"><span class="smallfont">@o.PrevMemberCount / @o.ProspectCount</span></td>
  }
  <td>@o.Schedule</td>
  <td><span id='ck-@o.Id' class='yesno pointer'>@(o.AllowSelfCheckIn ? "yes" : "no")</span></td>
  <td><span id='bs-@o.Id' class='bday pointer'>@o.BDayStart</span></td>
  <td><span id='be-@o.Id' class='bday pointer'>@o.BDayEnd</span></td>
  <td><a id='tt-@o.Id' href="/OrgSearch/ToggleTag/@o.Id" class="taguntag" title="Add to/Remove from Target Division">@o.Tag</a></td>
  <td><a id='mm-@o.Id' href="/OrgSearch/MainDiv/@o.Id" class="maindiv" title="Make Target Main Division">@(o.ChangeMain == true ? "Make Main" : "")</a></td>
</tr>