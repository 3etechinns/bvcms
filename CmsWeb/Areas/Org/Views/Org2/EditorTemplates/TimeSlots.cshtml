@model CmsData.Registration.TimeSlots
@using CmsWeb
@using UtilityExtensions
<div id="TimeSlotsList" class="control-group">
  <label class="control-label">Time Slots</label>
  <div class="controls">
    <div class="table not-wide w80">
      @Html.EditorFor(m => m.TimeSlotLockDays)
      @{ var listname = Html.NameFor2(m => m.list); }
      <div class="movable-list">
      @foreach (var f in Model.list)
      {
        f.Name = listname;
        @Html.EditorFor(m => f)
      }
      </div>
      <div class="newitem"><a href="/Org/NewTimeSlot/@listname"><img src="/Content/images/plus1.png" /> Add Time Slot</a></div>
    </div>
    @if (ViewBag.ShowHelp != false)
    {
      <div class="alert alert-info maxWidthMedium margin-top">@Html.Markdown(Model.Help)</div>
    }
  </div>
</div>