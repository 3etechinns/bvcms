@model CmsWeb.Models.RecreationModel
@{
    Layout = "/Views/Shared/SiteLayout.cshtml";
    ViewBag.Title = "Coaches";
}
<h2>Coaches</h2>
<table>
@foreach (var r in Model.FetchWannabeCoaches())
{ 
<tr>
    <td><a href="/Person/Index/@r.PeopleId">@r.Person.Name</a></td>
    <td>@r.Fname</td>
    <td>@r.Mname</td>
</tr>
}
</table>