@model System.Data.SqlClient.SqlDataReader
@{
    Layout = ViewExtensions2.TouchPointLayout();
    ViewBag.Title = "Sql View";
    ViewBag.PageHeader = "Sql View";
}
<div class="row">
    <div class="col-md-9">
        <div class="box box-responsive">
            <div class="box-content">
                <div class="table-responsive">
                    <table class="table table-striped">
                        <tbody>
                            @{ var read = Model.Read();}
                            @while (read)
                            {
                                <tr>
                                    @for (var i = 0; i < Model.FieldCount; i++)
                                    {
                                        <td>@Model[i].ToString()</td>
                                    }
                                </tr>
                                    read = Model.Read();
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>