@using CmsWeb;
@model CmsWeb.Models.TransactionsModel
@{
    Layout = "/Views/Shared/SiteLayout.cshtml";
    ViewBag.Title = "Transactions";
}
    <script type="text/javascript">
        $(document).ready(function () {
            $(".datepicker").datepicker();
            $('td.tip').tooltip({
                delay: 0,
                showURL: false,
                showBody: "|"
            });
            $('input[value="Filter"].submitbutton').live('click', function (ev) {
                ev.preventDefault();
                var f = $(this).closest('form');
                var q = f.serialize();
                $.post("/Manage/Transactions/List", q, function (ret) {
                    $(f).html(ret);
                    $('table.grid > tbody > tr:even').addClass('alt');
                });
                return false;
            });
            $('table.grid > tbody > tr:even').addClass('alt');
        });
</script>
<script src="/Scripts/Pager.js" type="text/javascript"></script>
<h2>Transactions</h2>
<form id="form" method="post" action="/Manage/Transactions/Export">
@{Html.RenderPartial("List", Model);}
</form>
