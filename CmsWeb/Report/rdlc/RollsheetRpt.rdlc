  <Code>Shared offset as Integer
Shared currentgroup as Object
Shared currentgrouptotalpages as Integer

Public Function GetGroupPageNumber(group as Object, pagenumber as Integer) _
    as Object
    If Not (group = currentgroup)
        offset = pagenumber - 1
        currentgroup = group
    End If
    Return pagenumber - offset
End Function

Public Function GetRecordType(group as Object) _
    as Object
    If Not (group = currentgroup)
         Return 1000001
    End If
    Return 1000002
End Function

Public Function GetGroupTotalPages _
  as Object
  Return currentgrouptotalpages
End Function

Public Function SetGroupTotalPages(TotalPages as Integer) _
  as Object
  currentgrouptotalpages = TotalPages
  Return currentgrouptotalpages
End Function</Code>
