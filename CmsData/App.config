<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="DbmlBuilderService" type="DbmlBuilder.DbmlBuilderSection, DbmlBuilder"
             requirePermission="false"/>
  </configSections>
  <DbmlBuilderService defaultProvider="CMS">
    <providers>
      <clear/>
      <add name="CMS" type="DbmlBuilder.SqlDataProvider, DbmlBuilder"
           connectionStringName="CMS"
           generatedNamespace="CmsData"/>
    </providers>
  </DbmlBuilderService>
  <appSettings/>
  <connectionStrings>
    <add name="CMS" connectionString="Data Source=.\SqlServer2008;Initial Catalog=CMS2;Integrated Security=True"
      providerName="System.Data.SqlClient" />
  </connectionStrings>
</configuration>